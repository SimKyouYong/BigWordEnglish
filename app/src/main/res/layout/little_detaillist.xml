<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="10dp"
    android:paddingBottom="10dp"
    tools:context="co.kr.bigwordenglish.MainActivity"
    android:orientation="vertical">

    <LinearLayout android:id="@+id/detaillist_lay" android:layout_width="match_parent" android:layout_height="wrap_content" android:orientation="horizontal" android:layout_below="@+id/top_lay" android:weightSum="10">
        <TextView android:id="@+id/no_tv" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1.8" android:layout_marginLeft="10dp" android:layout_marginRight="5dp" android:text=""/>
        <TextView android:id="@+id/word_tv" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="4.4" android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:text=""/>
        <TextView android:id="@+id/level_tv" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1.4" android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:text=""/>
        <ImageView android:id="@+id/pronunciation_tv" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1.2" android:layout_marginLeft="5dp" android:layout_marginRight="5dp" />
        <ImageView android:id="@+id/memo_tv" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1.2" android:layout_marginLeft="5dp" android:layout_marginRight="10dp" />

    </LinearLayout>
</LinearLayout>
